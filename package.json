{
  "name": "TechEdTools",
  "version": "1.0.0",
  "description": "A tools for organizing the list of technology education tools out there",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build",
    "start": "node server.js",
    "local-start": "nodemon --inspect server.js",
    "client": "cd client && npm start",
    "test": "jest",
    "develop": "concurrently --kill-others-on-fail \"npm run local-start\" \"npm run client\"",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix"
  },
  "author": "Zac Yauney",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^15.12.5",
    "bcryptjs": "^2.4.3",
    "chokidar": "^3.5.3",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^8.2.0",
    "ems": "^1.6.1",
    "express": "^4.17.1",
    "express-form-data": "^2.0.17",
    "joi": "^17.4.0",
    "mongoose": "^5.11.12",
    "multer": "^1.4.3",
    "package.json": "^2.0.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "miragejs": "^0.1.41"
  },
  "engines": {
    "node": "16.13.2",
    "npm": "8.4.0"
  }
}
